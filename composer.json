{
    "name": "guushuizen/nova-importer",
    "description": "Allows developers to create custom import functionality for their Nova resources",
    "keywords": [
        "laravel",
        "nova",
        "import",
        "card"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "GuusH\\NovaImporter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GuusH\\NovaImporter\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
